        \begin{enumerate}[label={$\square$}]


After

        \item \textbf{Data Access}
        \begin{enumerate}[label={$\square$}]
                \item Raw Eye Tracking Data
                \item Incomplete Data \hspace{2ex} $\square$ Low-Quality Data
                \item Installed Analysis Software
                \item Local Data Copy
                \item Data Formats
            \end{enumerate}
            
        \item \textbf{Data Processing}
        \begin{enumerate}[label={$\square$}]
                \item Data Transformations
                \item Data Cleansing
                \item Invalid Data
                \item Data Filtering
                \item Feature Extraction 
                \item Areas of Interest (AOIs)
                \item Clusters
                \item Correlations
                \item Mining Rules
                \item Data Dimensions
            \end{enumerate}
            
        \item \textbf{Analysis Tools}
        \begin{enumerate}[label={$\square$}]
                \item Integrated Tools
                \item External Tools
                \item Data Analysis Tools
                \item Visualization Tools
                \item Statistics Tools
            \end{enumerate}
            
        \item \textbf{Statistics}
        \begin{enumerate}[label={$\square$}]
                \item Statistical Tests
                \item Statistical Significance
                \item Value Distributions
            \end{enumerate}
            
        \item \textbf{Visualizations}
        \begin{enumerate}[label={$\square$}]
                \item Statistical Visualizations
                \item Point-Based Visualizations
                \item Region-Based Visualizations
                \item Human-Computer Interaction
                \item Visual Analytics
            \end{enumerate}

        \item \textbf{Advanced Algorithms}
        \begin{enumerate}[label={$\square$}]
                \item Deep Learning
                \item Scanpath Alignment
                \item Average Scanpath
                \item Dimensionality Reduction
            \end{enumerate}    
            
        \item \textbf{Initial Findings}
        \begin{enumerate}[label={$\square$}]
                \item Patterns
                \item Rules
                \item Anomalies
            \end{enumerate}
            
        \item \textbf{Matching Research Question}
        \begin{enumerate}[label={$\square$}]
                \item Hypotheses Confirmation
                \item Hypotheses Rejection
            \end{enumerate}
            
        \item \textbf{Conclusion Drawing}
        \begin{enumerate}[label={$\square$}]
                \item Analytical Reasoning
                \item Sense Making
                \item Decision Making
                \item Eye-Mind Hypothesis
                \item Study Goals
                \item Experimental Control
                \item Ecological Validity
            \end{enumerate}
            
        \item \textbf{Reporting and Dissemination}
        \begin{enumerate}[label={$\square$}]
                \item Scientific Paper
                %\item Publication
                \item Reproducibility
                \item Presentation or Talk
            \end{enumerate}
            
        \item \textbf{Open Data}
        \begin{enumerate}[label={$\square$}]
                \item Open Research Data
                \item Data Curation and Sharing
                \item Data Privacy and Ethics
            \end{enumerate}
            
        \end{enumerate}
